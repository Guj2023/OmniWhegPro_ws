  <version>0.0.0</version>
  <description>This package is to convert uart message to ros2 topics, using in OmniWhegPro Project</description>
  <maintainer email="guj2023@outlook.jp">Guj</maintainer>
  <license>No license, use it whatever you like</license>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
